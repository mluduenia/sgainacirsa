	<queryString>
		<![CDATA[select
a.id, a.fecha_alta, a.id_generacion, a.nivel, a.mm_promedio, a.cdad_estaciones_activas, a.cdad_estaciones_con_lluvia, a.cdad_estaciones,a.ultimas_x_horas_incluidas,c.id, c.nro, c.nombre,
(SELECT  lpad (cast((EXTRACT(DAY from fecha_alta)) as varchar), 2, '0')  || '/' ||
 lpad (cast((EXTRACT(MONTH from fecha_alta)) as varchar), 2, '0') || '/' ||
 EXTRACT(YEAR from fecha_alta)  || '+' ||
 lpad (cast((EXTRACT(HOUR from fecha_alta)) as varchar), 2, '0') || ':' ||
 lpad (cast((EXTRACT(minute from fecha_alta)) as varchar), 2, '0')) as fecha_string
from alertas a inner join cuencas c on c.id=a.id_cuenca
where
a.id_generacion=$P{idGeneracion}
order by c.nro asc]]>
	</queryString>
